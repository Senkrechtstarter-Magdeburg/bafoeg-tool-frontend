{
  "compileOnSave": false,
  "compilerOptions": {
    "baseUrl": "./",
    "outDir": "./dist/out-tsc",
    "sourceMap": true,
    "declaration": false,
    "downlevelIteration": true,
    "module": "esnext",
    "moduleResolution": "node",
    "experimentalDecorators": true,
    "importHelpers": true,
    "target": "es2015",
    "typeRoots": [
      "node_modules/@types"
    ],
    "lib": [
      "es2019",
      "dom"
    ],
    "paths": {
      "@models": [
        "src/app/models/index.ts"
      ],
      "@models/*": [
        "src/app/models/*"
      ],
      "@shared": [
        "src/app/shared/index.ts"
      ],
      "@shared/*": [
        "src/app/shared/*"
      ],
      "@questions": [
        "src/app/questions/index.ts"
      ],
      "@questions/*": [
        "src/app/questions/*"
      ]
    }
  }
}
